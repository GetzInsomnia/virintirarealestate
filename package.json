{
  "name": "zomzomproperty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node server.js",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "index:build": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/lib/search/indexBuilder.ts",
    "sitemap": "next-sitemap",
    "i18n:check": "ts-node scripts/i18n-check.ts || echo 'i18n check: warned'",
    "audit": "npm audit --omit=dev",
    "size": "size-limit",
    "update:rates": "node -e \"if (process.env.CI) { console.error('update:rates must not run in CI'); process.exit(1); }\" && ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/update-rates.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "archiver": "^5.3.2",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "eslint-plugin-react": "^7.37.5",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.12",
    "i18next": "^25.3.2",
    "mini-search": "npm:minisearch@^7.1.2",
    "minisearch": "^7.2.0",
    "negotiator": "^0.6.4",
    "next": "14.2.30",
    "next-i18next": "^15.4.2",
    "next-mdx-remote": "^4.4.1",
    "next-seo": "^6.8.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "rate-limiter-flexible": "^7.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.6.0",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^20.19.8",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.30",
    "jsdom": "^26.1.0",
    "prisma": "^5.22.0",
    "size-limit": "^11.1.4",
    "typescript": "^5.8.3"
  }
}
